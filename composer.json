{
    "name": "stoutlogic/understory-acf",
    "description": "ACF Extentions for Understory. Provides custom fields and option pages support for Understory.",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Steve Pfisterer",
            "email": "steve@stoutlogic.com"
        }
    ],
    "repositories": {
      "TGM": {
        "type": "git",
        "url": "https://github.com/StoutLogic/TGM-Plugin-Activation"
      }
  },
    "require": {
        "php": ">=5.4.0",
        "stoutlogic/understory": "^0.3.1|^0.4.0|^0.5.0",
        "stoutlogic/acf-builder": "^1.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.4.*",
        "tgmpa/tgm-plugin-activation": "dev-develop",
        "mockery/mockery": "^0.9.5",
        "php-mock/php-mock-mockery": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Understory\\ACF\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Understory\\ACF\\Tests\\": "tests/"
        }
    },
  "minimum-stability": "dev",

  "prefer-stable": true
}
